<odoo>
    <record id="estate_user_view_form" model="ir.ui.view">
        <field name="name">estate.user.view.form.inherit.user</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='account_security']" position="after">
                <page string="Real Estate Properties" name="estate_properties">
                    <field name="property_ids" widget="one2many_list"/>
                </page>
            </xpath>
        </field>
    </record>
</odoo>